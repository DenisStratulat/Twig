{% extends "../layouts/layout.twig" %}

{% set title = 'Content' %}

{% block content %}
	{% embed '../layouts/content/article.twig' %}
		{% block article %}
			<h1>Articles</h1>
			{{ parent() }}
		{% endblock %}
	{% endembed %}

	{% embed '../layouts/content/blog.twig' %}
		{% block blog %}
			<h1>Blogs</h1>
			{{ parent() }}
		{% endblock %}
	{% endembed %}
{% endblock %}